{
  "version": 2,
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}"
      },
      "warnings": {
        "deprecated": true,
        "unusedCli": true,
        "uninitialized": true
      }
    },
    {
      "name": "Debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "RelWithDebInfo",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      }
    },
    {
      "name": "Release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "MinSizeRel",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "MinSizeRel"
      }
    },


    {
      "name": "windows-default",
      "hidden": true,
      "inherits": "default",
      "displayName": "WINDOWS 64-BIT DEFAULT PRESET",
      "description": "Target Windows with the Visual Studio development environment.",
      "generator": "Ninja",
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "vendor": { "microsoft.com/VisualStudioSettings/CMake/1.0": { "hostOS": [ "Windows" ] } }
    },
    {
      "name": "windows-debug",
      "inherits": [ "windows-default", "Debug" ],
      "hidden": false,
      "displayName": "Windows x64 Debug"
    },
    {
      "name": "windows-relwithdebinfo",
      "inherits": [ "windows-default", "RelWithDebInfo" ],
      "hidden": false,
      "displayName": "Windows x64 Release w/ Debugging Symbols"
    },
    {
      "name": "windows-release",
      "inherits": [ "windows-default", "Release" ],
      "hidden": false,
      "displayName": "Windows x64 Release"
    },
    {
      "name": "windows-minsizerel",
      "inherits": [ "windows-default", "MinSizeRel" ],
      "hidden": false,
      "displayName": "Windows x64 Minimal Release"
    }
  ],



  "buildPresets": [
    {
      "name": "release-clean",
      "hidden": true,
      "cleanFirst": true,
      "verbose": false,
      "jobs": 4
    },
    {
      "name": "windows-x64-release",
      "inherits": "release-clean",
      "configurePreset": "windows-release",
      "displayName": "Clean Release"
    }
  ]
}
